class Solution {
    public String reversePrefix(String s, int k) {
       StringBuilder sb = new StringBuilder(s);
       int n = k/2;

        for(int i=0; i<n; i++){
            char temp = sb.charAt(i);
            sb.setCharAt(i ,sb.charAt(k-1-i));
            sb.setCharAt((k-1-i) , temp);
        }

        String result = sb.toString();
        
        return result;
    }
}
